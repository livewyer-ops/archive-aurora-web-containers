php_proc = Process(
    name="php_process",
    cmdline="php5-fpm -F")

php5_fpm = Task(
  name = 'PHP5 interpreter',
  processes = [php_proc],
  resources = Resources(cpu = 1, ram = 64*MB, disk=8*MB))

jobs = [
  Service(cluster = 'devcluster',
          environment = 'devel',
          role = 'php-interpreter',
          name = 'php5-fpm',
          task = php5_fpm,
          container = Container(docker = Docker(image = 'registry.livewyer.com/php5-fpm:2015-03-31-1510')))]
